{
  "name": "importer",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r dotenv/config src/server/startApp.ts",
    "start:db": "pg_ctl -D /usr/local/var/postgres start",
    "stop:db": "pg_ctl -D /usr/local/var/postgres stop",
    "watch": "nodemon --config ./nodemon.json",
    "ncu": "npm-check-updates",
    "build:client": "webpack",
    "init:deploy": "ts-node deploy/initDockerCompose.ts",
    "start:docker": "docker-compose -f docker-compose.prod.yml up -d"
  },
  "dependencies": {
    "@types/express": "4.16.1",
    "@types/express-session": "1.15.12",
    "@types/node": "12.0.2",
    "@types/node-fetch": "2.3.4",
    "@types/passport": "1.0.0",
    "@types/passport-github2": "1.2.4",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "express-session": "1.16.1",
    "node-fetch": "2.6.0",
    "nodemon": "1.19.1",
    "npm-check-updates": "3.1.9",
    "passport": "0.4.0",
    "passport-github2": "0.1.11",
    "passport-gitlab2": "5.0.0",
    "ts-node": "8.2.0",
    "tslint": "5.16.0",
    "typescript": "3.4.5",
    "webpack-cli": "3.3.2",
    "@types/nodegit": "0.24.6",
    "@types/pg": "7.4.14",
    "@types/pug": "2.0.4",
    "@types/rimraf": "2.0.2",
    "aws-sdk": "2.463.0",
    "lawgs": "1.0.4",
    "nodegit": "0.24.3",
    "pg": "7.11.0",
    "pug": "2.0.3",
    "rimraf": "2.6.3",
    "ts-loader": "6.0.1",
    "webpack": "4.32.2"
  }
}
