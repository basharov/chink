{
  "linterOptions": {
    "exclude": [
      "node_modules/**",
      "coverage/**",
      "package.json"
    ]
  },
  "rules": {
    "await-promise": false,
    "object-literal-sort-keys": false,
    "class-name": true,
    "curly": true,
    "linebreak-style": [
      true,
      "LF"
    ],
    "align": [
      true,
      "parameters",
      "statements"
    ],
    "comment-format": [
      true,
      "check-space"
    ],
    "completed-docs": false,
    "forin": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": [
      true
    ],
    "jsdoc-format": true,
    "label-position": true,
    "match-default-export-name": false,
    "max-line-length": [
      true,
      220
    ],
    "member-access": true,
    "member-ordering": [
      true,
      {
        "order": [
          "private-static-method"
        ]
      }
    ],
    "no-arg": true,
    "no-bitwise": true,
    "no-consecutive-blank-lines": true,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-empty-interface": false,
    "no-eval": true,
    "no-floating-promises": false,
    "no-http-string": false,
    "no-import-side-effect": [
      true,
      {
        "ignore-module": "(\\.html|\\.css)$"
      }
    ],
    "no-invalid-this": false,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-suspicious-comment": false,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    "no-unsafe-any": false,
    "no-unused-expression": true,
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "no-void-expression": false,
    "one-line": [
      true,
      "check-catch",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "radix": true,
    "react-this-binding-issue": false,
    "switch-default": true,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef": [
      true,
      "parameter",
      "property-declaration"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "type-literal-delimiter": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-module",
      "check-operator",
      "check-separator",
      "check-type",
      "check-typecast"
    ],
    "import-name": false,
    "jsx-boolean-value": false,
    "no-console": false,
    "no-submodule-imports": false,
    "semicolon": [
      true,
      "never"
    ],
    "strict-boolean-expressions": false,
    "trailing-comma": false,
    "ban": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-wrap-multiline": false,
    "no-any": false,
    "eofline": false,
    "export-name": [
      true,
      {
        "ignore-case": false,
        "ignore-module": "(\\.svg|\\.png)$"
      }
    ],
    "no-unused-variable": false,
    "no-relative-imports": false,
    "no-backbone-get-set-outside-model": false
  },
  "jsRules": {
    "object-literal-sort-keys": false,
    "no-console": false
  }
}
